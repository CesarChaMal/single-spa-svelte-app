{
  "name": "@cesarchamal/single-spa-svelte-app",
  "version": "1.0.18",
  "description": "Svelte microfrontend for Single-SPA demonstrating compile-time optimization",
  "main": "dist/single-spa-svelte-app.js",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "build:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode development",
    "build:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "start": "webpack serve --mode development --port 4211",
    "dev": "npm run start",
    "serve": "npm run build && npx http-server dist -p 4211 --cors",
    "lint": "eslint src --ext .js,.svelte --fix"
  },
  "devDependencies": {
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "cross-env": "^7.0.3",
    "webpack-dev-server": "^3.11.0",
    "svelte": "^3.59.2",
    "svelte-loader": "^3.1.8",
    "css-loader": "^3.4.2",
    "style-loader": "^1.1.3",
    "babel-loader": "^8.0.6",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "babel-eslint": "^10.0.3"
  },
  "dependencies": {
    "single-spa": "^5.9.0",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "rxjs": "^6.4.0"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cesarchamal/single-spa-svelte-app.git"
  },
  "keywords": [
    "single-spa",
    "svelte",
    "npm",
    "webpack"
  ],
  "author": "Cesar Francisco Chavez Maldonado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cesarchamal/single-spa-svelte-app/issues"
  },
  "homepage": "https://github.com/cesarchamal/single-spa-svelte-app#readme"
}
